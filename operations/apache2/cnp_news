<VirtualHost *:8181>
    Options None

    AddHandler mod_python .py
    PythonDebug On

    # Logfiles
    ErrorLog  /var/log/cnp_news_error.log
    CustomLog /var/log/cnp_news_access.log combined

    <Location "/">
        SetHandler python-program
        PythonHandler django.core.handlers.modpython
        SetEnv DJANGO_SETTINGS_MODULE settings
        PythonPath "['/var/www/cnp_news'] + sys.path"
        PythonInterpreter cnp_news
    </Location>
</VirtualHost>
